{
  "expo": {
    "name": "itc-service",
    "slug": "itc-service",
    "version": "1.0.0",
    "icon": "./assets/itec.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-firebase/app"
    ],
    "android": {
      "package": "com.itcservice",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive.png",
        "backgroundColor": "#fff"
      }
    },
    "ios": {
      "bundleIdentifier": "com.itcservice",
      "googleServicesFile": "./GoogleService-Info.plist"
    }
  }
}
